<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileQR.MainPage"
                >

     <StackLayout Orientation="Horizontal">
        <StackLayout HorizontalOptions="Start" VerticalOptions="Start">
            <Button Text="Генерация QR" Clicked="GenerateQR_Clicked"/>
        </StackLayout>
        <StackLayout HorizontalOptions="End" VerticalOptions="Start">
            <Button Text="Вложенный 2"/>
        </StackLayout>
        <StackLayout HorizontalOptions="Start" VerticalOptions="End">
            <Button AnchorX="40" AnchorY="60" Text="SCAN" Clicked="Button_Clicked"/>
        </StackLayout>
        
    </StackLayout>

</ContentPage>
